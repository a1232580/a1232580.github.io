<!doctype html>
<html>

<head>
  <meta charset="utf-8" />
  <title>Secret Master Mission Editor</title>
  <script>
    function getExplorer() {
      let explorer = window.navigator.userAgent;
      let ie11 = ("ActiveXObject" in window);
      function compare(s) {
        return (explorer.indexOf(s) >= 0);
      }
      if (ie11 || compare("MSIE") || compare("Edge")) {
        return 'ie';
      } else if (compare("Firefox")) {
        return 'Firefox';
      } else if (compare("Chrome")) {
        return 'Chrome';
      } else if (compare("Opera")) {
        return 'Opera';
      } else if (compare("Safari")) {
        return 'Safari';
      } else {
        return 'Unknown';
      }
    }

    if (getExplorer() === 'ie') {
      alert('To ensure your asset security, Chrome, Firefox or Safari is recommended.');
    } 
  </script>
</head>

<body style="background-color: rgb(208, 222, 243);">
  <div id="root"></div>
</body>

</html>